#include <iostream>
#include <algorithm>
#include <stack>
#include <queue>
#include <string>
using namespace std;

/* run this program using the console pauser or add your own getch, system("pause") or input loop */
string str;
int cnt[26];
int big = 0;
char alpa;
int st = 0;
int main()
{
	cin >> str;
	int length = str.length(); 
	
	for(int i = 0; i < length; i++)
	{
		if(str[i] >= 'a')
		{
			cnt[str[i] - 'a']++;	
		}	
		else
		{
			cnt[str[i] - 'A']++;
		}
	}
	for(int i = 0; i < 26; i++)
	{
		if(cnt[i] > big)
		{
			big = cnt[i];
			alpa = str[i];
		}	
	}
	for(int i = 0; i < 26; i++)
	{
		if(big == cnt[i])
		{
			st++;
			if(st >= 2)
			{
				printf("?");
				return 0;
			}
		}
	}
	alpa = alpa - 32;
	printf("%c",alpa);	
	return 0;
}
