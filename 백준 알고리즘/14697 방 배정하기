#include <iostream>
#include <algorithm>
using namespace std;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) 
{
	int a = 0,b = 0,c = 0,s = 0;
	int x = 0,y = 0,z = 0;
	scanf("%d %d %d %d",&a, &b, &c, &s);
	for(int i = 0; i <= s; i++)
	{
		for(int j = 0; j <= s; j++)
		{
			for(int k = 0; k <= s; k++)
			{
				if((i * a)+ (j * b) + (k * c)== s)
				{
					printf("1");
					return 0;
				}
			}
		}
	}
	printf("0");
	return 0;
}
